def table(number):
    for i in range(1,11):
        total = number * i
        print(number, "*", i, "=",total)

table(int(input("enter table number for loop\n")))

def number1(table1):
    count = 1
    while count < 11:
        total = table1 * count
        print(table1, "*", count, "=",total)
        count = count + 1

number1(int(input("enter table number while loop\n")))
